{
	"README" 	: "DO NOT EDIT THIS FILE, IT WILL BE OVERWRITTEN WITHOUT WARNING DURING UPDATES",
	"version"	: "0.6",
	"targets" 	: {
		"diaspora"	: {
			"url"		: "{{pod}}/bookmarklet?url={{url}}&title={{title}}&notes={{tags}}{{text}}&v=1&noui=1&jump=doclose"
		},
		"facebook"	: {
			"url"		: "https://www.facebook.com/sharer/sharer.php?m2w&u={{url}}&t={{title}}",
			"comment" 	: "m2w parameter forces desktop mode, it has better privacy and image controls"
		},
		"twitter"	: {
			"url"		: "https://twitter.com/intent/tweet?url={{url}}&text={{title}}+{{tags}}"
		},
		"gplus"		: {
			"url"		: "https://plus.google.com/share?url={{url}}"
		},
		"gbookmarks": {
			"url"		: "https://www.google.com/bookmarks/mark?op=edit&bkmk={{url}}&title={{title}}&annotation=",
			"comment"	: "annotation can be used for notes when supported"
		},
		"delicious" : {
			"url"		: "http://delicious.com/save?url={{url}}&title={{title}}&notes={{text}}"
		},
		"linkedin"  : {
			"url"		: "http://www.linkedin.com/shareArticle?mini=true&url={{url}}&title={{title}}",
			"comment"	: "summary parameter is available for notes when supported"
		},
		"gtranslate": {
			"url"		: "http://translate.google.com/translate?u={{url}}&sl=auto"
		},
		"tumblr"	: {
			"url"		: "http://www.tumblr.com/share?v=3&u={{url}}&s="
		},
		"dzone"		: {
			"url"		: "http://www.dzone.com/links/add.html?url={{url}}&title={{title}}"
		},
		"pingfm"	: {
			"url"		: "http://ping.fm/ref/?link={{url}}&title={{title}}+{{tags}}"
		},
		"digg"		: {
			"url"		: "http://www.digg.com/submit?phase=2&url={{url}}&title={{title}}"
		},
		"stumble"	: {
			"url"		: "http://www.stumbleupon.com/submit?url={{url}}&title={{title}}"
		}
	}
}
